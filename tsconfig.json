{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "baseUrl": ".",
        "declaration": true,
        "declarationMap": true,
        "disableSizeLimit": true,
        "emitDecoratorMetadata": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "importHelpers": true,
        "jsx": "preserve",
        "lib": [
            "ES2015",
            "ES2016",
            "ES2017",
            "ES2018",
            "ES2019",
            "ES2020",
            "ESNext",
            "DOM"
        ],
        "listEmittedFiles": true,
        "listFiles": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "noErrorTruncation": true,
        "inlineSourceMap": true,
        "inlineSources": true,
        "preserveConstEnums": true,
        "skipLibCheck": true,
        "allowJs": true,
        "strict": false,
        "stripInternal": false,
        "strictBindCallApply": true,
        "strictPropertyInitialization": false,
        "target": "ES5",
        "typeRoots": [
            "node_modules/@types"
        ],
        "types": [
            "webpack-env"
        ],
        "paths": {
            "#": [
                "projects/index.ts"
            ],
            "#/*": [
                "projects/*"
            ],
            "@/*": [
                "src/*"
            ]
        }
    },
    "exclude": [
        "node_modules",
        "scripts",
        "dist"
    ]
}
